import "runtime/print.tan";
import "runtime/debug.tan";

pub fn main(argc: int, argv: char**) : int {
    assert(!0.0);
    assert(!1.0 == 0.);
    assert(1 < 2);
    assert(1.1 < 2.2);
    assert(1.0 + 2.0 > 2.8);
    assert(3.0 > 2.1);
    assert(1. > -2.0);
    assert(1 != 0);
    assert(!(1 == 0));
    assert(1.0 + 2.0 == 3.);
    assert(1 + 3 == 2 + 2);
    assert(1.0 + 3.0 == 2. + 2.0);

    var b = 1 + 1 == 2;
    assert(b == true);
    assert(b != false);

    print("SUCCESS\n");
    return 0;
}

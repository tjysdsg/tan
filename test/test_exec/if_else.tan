import std;

fn sign(a: int) : int {
    var ret: int = 0;
    if (a > 0) {
        ret = 1;
    } else if (a < 0) {
        ret = -1;
    }
    return ret;
}

fn else_if(c: i32) : i32 {
    if (c == 0) {
        return 0;
    } else if (c == 1) {
        return 1;
    } else {
        return 2;
    }
}

pub fn main(argc: int, argv: char**) : int {
    if (argc > 0) {
        assert(true);
    } else {
        assert(false);
    }
    assert(sign(-10) == -1);
    assert(sign(0) == 0);
    assert(sign(12) == 1);
    assert(else_if(1) == 1);
    print("SUCCESS\n");
    return 0;
}

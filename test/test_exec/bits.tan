import std;

pub fn main(argc: int, argv: char**) : int {
    var a = 0;
    var b = 1;
    assert(a | b);
    assert(!(a & b));

    a = 1;
    b = 2;
    var res = a | b;
    assert(res);
    assert(!(a & b));

    a = 3;
    b = 2;
    assert(a | b);
    assert(a & b);
    assert(a & b == 2);

    a = ~0;
    assert(a);
    assert(a == -1);

    a = ~-2;
    assert(a == 1);

    print("SUCCESS\n");
    return 0;
}
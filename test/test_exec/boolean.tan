import std;

pub fn main(argc: int, argv: char**) : int {
    assert(!0.0);
    assert(!1. == false);
    assert(!0. == true);
    assert(!5 == false);
    assert(3 == true);
    assert(1 < 2);
    assert(1.1 < 2.2);
    assert(1.0 + 2.0 > 2.8);
    assert(3.0 > 2.1);
    assert(1. > -2.0);
    assert(1 != 0);
    assert(!(1 == 0));
    assert(1.0 + 2.0 == 3.);
    assert(1 + 3 == 2 + 2);
    assert(1.0 + 3.0 == 2. + 2.0);

    var a = 20;
    var b = 20;
    assert(a <= b || a < b);
    b = 21;
    assert(a <= b && a < b);

    var b = 1 + 1 == 2;
    assert(b == true);
    assert(b != false);

    print("SUCCESS\n");
    return 0;
}
